{
  "name": "homeportal",
  "version": "1.0.0",
  "description": "Frontend for my home APIs",
  "engines": {
    "node": "8.9.x"
  },
  "scripts": {
    "start": "node server",
    "heroku-postbuild": "cd client/ && npm install && npm install --only=dev --no-shrinkwrap && npm run build",
    "dev-client": "cd client && npm start",
    "dev-server": "nodemon server.js",
    "dev": "concurrently --kill-others-on-fail \"npm run dev-server\" \"npm run dev-client\""
  },
  "cacheDirectories": [
    "node_modules",
    "client/node_modules"
  ],
  "dependencies": {
    "apollo-boost": "^0.1.3",
    "apollo-link-persisted-queries": "^0.2.0",
    "apollo-link-ws": "^1.0.7",
    "dotenv": "^5.0.0",
    "express": "^4.14.1",
    "graphql": "^0.13.2",
    "http-proxy-middleware": "^0.17.4",
    "react-apollo": "^2.0.4",
    "serialize-javascript": "^1.4.0",
    "subscriptions-transport-ws": "^0.9.6"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:dariusbakunas/homeportal.git"
  },
  "keywords": [
    "node",
    "heroku",
    "create-react-app",
    "react"
  ],
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^3.5.1",
    "nodemon": "^1.15.1",
    "webpack-graphql-loader": "^0.1.0"
  }
}
